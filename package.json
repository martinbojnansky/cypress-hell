{
  "name": "cypress-snapshots",
  "version": "0.0.4",
  "description": "",
  "main": "dist",
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "npm-run-all clean -p watch serve cypress",
    "clean": "rimraf dist",
    "compile": "tsc",
    "watch": "npm run compile -- -w",
    "serve": "node index.js",
    "cypress": "cypress open",
    "cypress-ci": "cypress run",
    "update-snapshots": "npm run cypress-ci -- --env updateSnapshots=true",
    "publish-package": "npm run clean && npm run compile && npm publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/martinbojnansky/cypress-snapshots.git"
  },
  "keywords": [
    "cypress",
    "testing",
    "regression",
    "testing",
    "e2e",
    "screenshots"
  ],
  "author": "Martin Bojnansky",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/martinbojnansky/cypress-snapshots/issues"
  },
  "homepage": "https://github.com/martinbojnansky/cypress-snapshots#readme",
  "devDependencies": {
    "@types/cypress": "^1.1.3",
    "@types/jest": "^26.0.23",
    "@types/node": "^15.12.2",
    "@types/pixelmatch": "^5.2.3",
    "cypress": "^7.5.0",
    "npm-run-all": "^4.1.5",
    "typescript": "^4.3.2"
  },
  "dependencies": {
    "pixelmatch": "^5.2.1"
  }
}
